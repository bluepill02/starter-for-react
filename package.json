{
  "name": "react-starter-kit-for-appwrite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:all": "concurrently \"npm run dev:emulator\" \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "dev:emulator": "bash scripts/start-emulator.sh",
    "dev:seed": "cd apps/web && npm run seed",
    "dev:health": "bash scripts/wait-for-ready.sh",
    "dev:setup": "npm run dev:emulator && npm run dev:seed",
    "dev:reset": "docker-compose -f docker-compose.emulator.yml down -v && npm run dev:setup",
    "test:e2e": "cd packages/tests && npm run test:e2e",
    "test:smoke": "cd packages/tests && npm run test:smoke",
    "test:unit": "jest",
    "build:web": "cd apps/web && npm run build",
    "build:api": "cd apps/api && npm run build",
    "type-check": "tsc --noEmit --project .",
    "emulator:bootstrap": "bash scripts/bootstrap-project.sh",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:env": "powershell -ExecutionPolicy Bypass -File scripts/test-basic.ps1"
  },
  "dependencies": {
    "@appwrite.io/pink-icons": "^1.0.0",
    "@tailwindcss/vite": "^4.0.14",
    "appwrite": "^17.0.0",
    "dotenv": "^17.2.3",
    "node-appwrite": "^20.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^4.0.14",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "vite": "^6.0.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
